{
  "name": "@azen-sh/core",
  "version": "0.1.0",
  "module": "./index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun build index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit",
    "db:generate": "bun --env-file=.env drizzle-kit generate",
    "db:migrate": "bun --env-file=.env run db/migrate.ts",
    "db:studio": "bun --env-file=.env drizzle-kit studio"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "drizzle-kit": "^0.31.9",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@azen-sh/types": "workspace:*",
    "@qdrant/js-client-rest": "^1.17.0",
    "bullmq": "^5.70.1",
    "drizzle-orm": "^0.45.1",
    "ioredis": "^5.9.3",
    "neo4j-driver": "^6.0.1",
    "openai": "^6.22.0",
    "postgres": "^3.4.8",
    "zod": "^4.3.6"
  }
}